#include <iostream>
using namespace std;

string swapString(string n)
{
    string swapped;

    for (int i = n.size() - 1; i >= 0; i--)
    {
        swapped = swapped + n[i];
    }
    return swapped;
}

int main()
{
    string s,t;
    cin>>s>>t;

    if(t== swapString(s))
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    }

    return 0;
}
